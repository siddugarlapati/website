# Server Configuration
NODE_ENV=production
PORT=3001
FRONTEND_URL=https://your-frontend-domain.com

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Google Gemini API
GEMINI_API_KEY=your-google-gemini-api-key-here

# Admin Credentials (hash these passwords!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD_HASH=$2a$10$your-bcrypt-hashed-password-here

# Client Credentials (example)
CLIENT_USERNAME=skyline
CLIENT_PASSWORD_HASH=$2a$10$your-bcrypt-hashed-password-here

# Supabase (Optional - for production database)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key
